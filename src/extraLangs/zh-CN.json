{
  "languageTag": "zh-CN",
  "fileDescription": "skymen_smartjoystick 的字符串。",
  "text": {
    "plugins": {
      "skymen_smartjoystick": {
        "name": "智能摇杆",
        "description": "一个智能摇杆插件",
        "help-url": "https://www.construct.net",
        "aceCategories": {
          "general": "常规"
        },
        "properties": {
          "texture": {
            "name": "纹理",
            "desc": "点击编辑对象的纹理。",
            "link-text": "编辑"
          },
          "size": {
            "name": "大小",
            "desc": "点击将对象设置为与其图片相同的大小。",
            "link-text": "设为1:1"
          },
          "touch-event": {
            "name": "触摸事件",
            "desc": "在触摸开始和结束时执行的操作",
            "items": {
              "None": "无",
              "Fade": "淡出",
              "Reset Position": "重置位置"
            }
          },
          "initial-state": {
            "name": "初始状态",
            "desc": "如果为淡出，决定摇杆是否初始可见。",
            "items": {
              "Visible": "可见",
              "Invisible": "不可见"
            }
          },
          "mode": {
            "name": "模式",
            "desc": "无：像普通摇杆一样工作。智能：如果手柄太远则跟随手柄。生成：在半径范围内点击会生成摇杆，然后像普通摇杆一样工作。",
            "items": {
              "None": "无",
              "Smart": "智能",
              "Spawn": "生成"
            }
          },
          "radius": {
            "name": "半径",
            "desc": "仅在智能或生成模式下。决定摇杆被触发的半径。"
          },
          "use-mouse-input": {
            "name": "使用鼠标输入",
            "desc": "是否使用鼠标输入或仅使用触摸输入",
            "items": {
              "Yes": "是",
              "No": "否"
            }
          },
          "fade-time": {
            "name": "淡出时间",
            "desc": "仅当触摸事件设置为淡出时。时间以毫秒为单位。"
          }
        },
        "actions": {
          "bind-handle": {
            "list-name": "绑定手柄",
            "display-text": "将 {0} 绑定到摇杆手柄",
            "description": "绑定手柄",
            "params": {
              "handle": {
                "name": "手柄",
                "desc": "要绑定的手柄。"
              }
            }
          },
          "set-event": {
            "list-name": "设置触摸事件",
            "display-text": "将触摸事件设置为 {0}",
            "description": "设置触摸事件",
            "params": {
              "event": {
                "name": "事件",
                "desc": "要设置的事件",
                "items": {
                  "none": "无",
                  "fade": "淡出",
                  "spawn": "生成"
                }
              }
            }
          },
          "set-mode": {
            "list-name": "设置模式",
            "display-text": "将模式设置为 {0}",
            "description": "设置模式",
            "params": {
              "mode": {
                "name": "模式",
                "desc": "要设置的模式",
                "items": {
                  "none": "无",
                  "smart": "智能",
                  "spawn": "生成"
                }
              }
            }
          },
          "set-radius": {
            "list-name": "设置半径",
            "display-text": "将半径设置为 {0}",
            "description": "设置半径",
            "params": {
              "radius": {
                "name": "半径",
                "desc": "要设置的半径"
              }
            }
          }
        },
        "conditions": {
          "is-dragging": {
            "list-name": "正在拖动",
            "display-text": "正在拖动",
            "description": "当摇杆被使用时为真",
            "params": {}
          },
          "on-drag-start": {
            "list-name": "开始拖动时",
            "display-text": "开始拖动时",
            "description": "当摇杆开始被使用时触发",
            "params": {}
          },
          "on-drag-stop": {
            "list-name": "结束拖动时",
            "display-text": "结束拖动时",
            "description": "当摇杆停止被使用时触发",
            "params": {}
          }
        },
        "expressions": {
          "joystick-angle": {
            "translated-name": "JoystickAngle",
            "description": "返回摇杆的角度",
            "params": {}
          },
          "joystick-dir-x": {
            "translated-name": "JoystickDirX",
            "description": "返回摇杆的X方向位移",
            "params": {}
          },
          "joystick-dir-y": {
            "translated-name": "JoystickDirY",
            "description": "返回摇杆的Y方向位移",
            "params": {}
          },
          "joystick-distance": {
            "translated-name": "JoystickDistance",
            "description": "返回摇杆的距离",
            "params": {}
          },
          "joystick-force": {
            "translated-name": "JoystickForce",
            "description": "返回摇杆的力度",
            "params": {}
          },
          "joystick-force-x": {
            "translated-name": "JoystickForceX",
            "description": "返回摇杆的X方向力度",
            "params": {}
          },
          "joystick-force-y": {
            "translated-name": "JoystickForceY",
            "description": "返回摇杆的Y方向力度",
            "params": {}
          },
          "joystick-radius": {
            "translated-name": "JoystickRadius",
            "description": "返回摇杆的半径",
            "params": {}
          },
          "joystick-x": {
            "translated-name": "JoystickX",
            "description": "返回摇杆的X坐标",
            "params": {}
          },
          "joystick-y": {
            "translated-name": "JoystickY",
            "description": "返回摇杆的Y坐标",
            "params": {}
          }
        }
      }
    }
  }
}